<td style="background:{{ background }};padding:{{ padding_v }}px {{ padding_h }}px {{ padding_v }}px {{ padding_h }}px">
    <table  width="100%" border="0" cellpadding="0" cellspacing="0">
        <tr>
          {% for row in rows %}
              <td width="{{ 100*row.width /12 }}%">
                  <table  width="100%" border="0" cellpadding="0" cellspacing="0">
                      <tr>
                        {{ row.content | raw}}
                      </tr>
                  </table>
              </td>
          {% endfor %}
        </tr>
    </table>
</td>


