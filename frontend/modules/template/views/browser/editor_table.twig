<div>
  <ul class="sort_list sort_list-{{ table.type }}">
    {% for k in items %}
      <li data-name="{{ k }}" {% if k in table.hidden %}class="el_hidden"{% endif %}>
        {{ table.list[k] }}
        <i class="glyphicon glyphicon-eye-open hide_show"></i>
        <i class="glyphicon glyphicon-eye-close hide_show"></i>
      </li>
    {% endfor %}
  </ul>
</div>