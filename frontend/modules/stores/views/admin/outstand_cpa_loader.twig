{% for key, item in app.params.outstand_cpa if item.route == store.route and item.file_loader %}
    <div class="row">
        <div class="col-md-12">
            <h4> Загрузка данных CPA из файла</h4>

            {% set form = active_form_begin({'options' : {'enctype' : 'multipart/form-data'}, 'action': '/admin/stores/outstandcpa'}) %}

                {{ form.field(fileimport, 'store').hiddenInput({value: store.uid}).label(false)|raw }}

                {{ form.field(fileimport, 'cpa').hiddenInput({value: key}).label(false)|raw }}


                {{ form.field(fileimport, 'file').fileInput().label(false)|raw }}

                <input type="submit" class="btn btn-primary" value="Загрузить">

            {{ active_form_end() }}
        </div>
    </div>
{% endfor %}
