<h2 class="content-header">{{ t('main','coupons_all_expired') }}</h2>
<div  class="catalog-coupons margin-half">
  {% for coupon in coupon_ended %}
    <div class="accordion">
      <div class="accordion-control accordion-title">{{ coupon.name }}</div>
      <div class="accordion-content">
        {% if  coupon.description%}
          {{ coupon.description }}
        {% else %}
          {{ t("main","no_description") }}
        {% endif %}
      </div>
    </div>
  {% endfor %}
</div>