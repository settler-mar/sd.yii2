{{ use('yii/bootstrap') }}
{{ use('yii\bootstrap\ActiveForm') }}
{{ register_asset_bundle('frontend/assets/AppAsset') }}
{{   void(this.beginPage()) }}

<!DOCTYPE html>
<html lang="ru-RU" class="no-js">
<head>
    <title>{{ html.encode(this.title) }}</title>
    <base href="/">


  {% if description %}
    <meta name="description" content="{{description}}">
  {% endif %}

  {% if keywords %}
    <meta name="keywords" content="{{keywords}}">
  {% endif %}

  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">


  <link rel="apple-touch-icon" href="/apple-touch-icon.png">
  <link rel="icon" type="image/png" href="/favicon-32x32.png" sizes="32x32">
  <link rel="icon" type="image/png" href="/favicon-16x16.png" sizes="16x16">
  <link rel="icon" type="image/svg+xml" href="/favicon.svg">
  <link rel="shortcut icon" href="/favicon.ico">

  {% if pagination_tags.prev_page is not null %}
    <link rel="prev" href="{{site_url}}{{pagination_tags.prev_page}}">
  {% endif %}
  {% if pagination_tags.next_page is not null %}
    <link rel="next" href="{{site_url}}{{pagination_tags.next_page}}">
  {% endif %}


  {% for style in exclusionCSSheader %}
    <link rel="stylesheet" href="{{style}}">
  {% endfor %}

    {% for css_link in minifyCSS %}
        <link rel="stylesheet" href="{{css_link}}">
    {% endfor %}

  {% for scriptH in exclusionJSheader %}
    <script src="{{scriptH}}"></script>
  {% endfor %}

    {{ html.csrfMetaTags | raw }}
    {{   void(this.head) }}
</head>
<body id="top">
{{   void(this.beginBody()) }}
<header class="navbar navbar-default">
  <div class="container-fluid">
    <div class="navbar-header">
      <a href="/admin" ><img src="/favicon-32x32.png" width="32" alt="secretdiscounter logo"></a>
    </div>
    <div id="navbar" class="navbar-collapse collapse">
      <ul class="nav navbar-nav">
        <li class="no-icon has-children">
          <a href="/admin/stores">Магазины</a>
          <ul>
            <li><a href="/admin-categories/stores">Категории</a></li>
          </ul>
        </li>
        <li class="no-icon  has-children" style="display:none">
          <a href="/admin/actions">Акции</a>
          <ul>
            <li><a href="/admin/promo">Промокоды</a></li>
          </ul>
        </li>
        <li class="no-icon has-children">
          <a href="/admin/coupons">Купоны</a>
          <ul>
            <li><a href="/admin-categories/coupons">Категории</a></li>
            {% for config_key,config  in app.params.configs  %}
              <li><a href="/admin/configs?config={{ config_key }}">{{ config.title }}</a></li>
            {% endfor %}
          </ul>
        </li>
        <li class="no-icon"><a href="/admin/products">Продукты</a></li>
        <li class="no-icon has-children">
          <a href="/admin/users">Пользователи</a>
          <ul>
            <li><a href="/admin/payments">Платежи</a></li>
            <li><a href="/admin/users/action">Акция</a></li>
            <li><a href="/admin-social/users">Социальные сети</a></li>
            <li><a href="/admin/notification">Бонусы и уведомления</a></li>
            <li><a href="/admin/reviews">Отзывы</a></li>
            <li><a href="/admin/transitions">Переходы</a></li>
            <li><a href="/admin/withdraw">Вывод денег</a></li>
            <li><a href="/admin/users/export">Экспорт</a></li>
            <li><a href="/admin/promo">Промокоды</a></li>
          </ul>
        </li>
        <li class="no-icon has-children">
          <a href="/admin/charity">Благотворительность</a>
          <ul>
            <li><a href="/admin/funds">Фонды</a></li>
            <li><a href="/admin/charity">Пожертвования</a></li>
          </ul>
        </li>
        <li class="no-icon has-children">
          <a href="">Система</a>
          <ul>
            <li class="no-icon"><a href="/admin/meta">Метаданные</a></li>
            <li class="no-icon"><a href="/admin/constants">Константы</a></li>
            <li class="no-icon"><a href="/admin/banners">Баннеры</a></li>
            <li class="no-icon"><a href="/admin/slider">Слайдер</a></li>
          </ul>
        </li>
        <li class="no-icon has-children">
          <a href="/admin/b2b_users">B2B</a>
          <ul>
            <li><a href="/admin/b2b_users">Пользователи</a></li>
            <li><a href="/admin/b2b_content">Страницы</a></li>
          </ul>
        </li>
      </ul>
      <ul class="nav navbar-nav navbar-right">
        <li class="no-icon has-children">
          <a>Операции</a>
          <ul>
            <li class="action-btn"><a class="ajaxFormOpen" href="#admin/cache/clear">Обновить сайт</a></li>
            <li class="action-btn"><a href="/admin/language">Тест языка</a></li>
          </ul>
        </li>
        <li class="no-icon">
          <a href="/logout" onclick="location.href = '/logout'">Выход</a>
        </li>
      </ul>
    </div>
  </div>
</header>

<header class="cd-main-header" style="display: none;">
  <a href="/admin" ><img src="/favicon-32x32.png" width="32" alt="secretdiscounter logo"></a>


  <nav class="cd-nav">
    <ul class="cd-top-nav">
      <li class="has-children account logout-menu">
        <a href="/logout" onclick="location.href = '/logout'">Выход</a>
      </li>
    </ul>
  </nav>
</header>
<main>
  {{ content|raw }}
</main>
<script src="{{minifyJS}}"></script>
{% for scriptF in exclusionJSfooter %}
  <script src="{{scriptF}}"></script>
{% endfor %}

{% for script in minifyJS %}
    <script src="{{script}}"></script>
{% endfor %}
{{   void(this.endBody()) }}
{{ Notification() | raw}}
</body>
</html>
{{   void(this.endPage()) }}