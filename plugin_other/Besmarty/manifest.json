{
   "background": {
      "persistent": true,
      "scripts": [ "background/background.js" ]
   },
   "browser_action": {
      "default_icon": "$assets/webpack-16.png",
      "default_popup": "popup/index.html",
      "default_title": "Besmarty cashback"
   },
   "content_scripts": [ {
      "css": [ "content/content.css" ],
      "js": [ "content/content.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "default_locale": "ru",
   "description": "Besmarty cashback chrome plugin",
   "homepage_url": "http://besmarty.ru",
   "icons": {
      "128": "$assets/webpack-128.png",
      "16": "$assets/webpack-16.png",
      "48": "$assets/webpack-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Besmarty.ru",
   "permissions": [ "tabs", "cookies", "storage", "alarms" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.0.0",
   "web_accessible_resources": [ "content/images/content-logo.png", "content/images/content-bg.png", "content/images/i-warn.svg", "content/images/i-close-gray.svg", "content/images/i-close-pink.svg", "content/content.css" ]
}
